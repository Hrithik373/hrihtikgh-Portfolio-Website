{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "index.js" }],
  "env": {
    "SANITY_PROJECT_ID": "@sanity_project_id",
    "SANITY_DATASET": "@sanity_dataset"
  },
  "build": {
    "command": "npm run build",
    "functions": {
      "api/**/*.{js,ts}": { "runtime": "@vercel/node@16" }
    }
  }
}