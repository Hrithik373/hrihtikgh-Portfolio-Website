{
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "env": {
    "SANITY_PROJECT_ID": "@sanity_project_id",
    "SANITY_DATASET": "@sanity_dataset"
  },
  "env": {
    "SANITY_PROJECT_ID": "@sanity_project_id",
    "SANITY_DATASET": "@sanity_dataset"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "dev": {
    "env": {
      "NODE_ENV": "development"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "scripts": {
    "build": "next build",
    "start": "next start"
  }
}